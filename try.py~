import sys
import urllib
import urllib2
from lxml import etree
# coding: utf-8


if __name__ == '__main__':

	infile=open('province.txt','r')
	outfile=open('data.txt','a')
	data=infile.read()
	page = etree.HTML(data.decode('utf-8'))
	ps = page.xpath("//option")

	for p in ps:
		outfile.write(p.xpath('string(.)').encode('utf-8') + ' ')
#		outfile.write(p.attrib.get('value').encode('utf-8')+'\n')
	outfile.write('\n')
	for p in ps:
		outfile.write(p.attrib.get('value').encode('utf-8') + ' ')
	infile.close()
	outfile.close()




